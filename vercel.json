{
  "redirects": [
    {
      "source": "/blog",
      "destination": "https://blog.lucasoliveira.io",
      "statusCode": 301
    }
  ],
  "rewrites": [
    {
      "source": "/my-stuff",
      "destination": "https://personal-stuff.vercel.app/my-stuff"
    },
    {
      "source": "/my-stuff/",
      "destination": "https://personal-stuff.vercel.app/my-stuff"
    },
    {
      "source": "/my-stuff/:path*",
      "destination": "https://personal-stuff.vercel.app/my-stuff/:path*"
    },
    {
      "source": "/",
      "destination": "https://www.lucasoliveira.io"
    },
    {
      "source": "/:path*",
      "destination": "https://www.lucasoliveira.io/:path*"
    }
  ]
}
